{
  "name": "dash_kaleido",
  "version": "0.1.2",
  "description": "Kaleido offers powerful tab management for dash applications",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/LudwigAJ/dash-kaleido.git"
  },
  "bugs": {
    "url": "https://github.com/LudwigAJ/dash-kaleido/issues"
  },
  "homepage": "https://github.com/LudwigAJ/dash-kaleido",
  "scripts": {
    "start": "webpack serve --config ./webpack.serve.config.js --open",
    "build:js": "webpack --mode production",
    "build:js:dev": "webpack --mode development",
    "build:backends": "dash-generate-components ./src/lib/components dash_kaleido -p package-info.json -k DashKaleido,KaleidoManager,KaleidoTab --ignore \\.test\\. --ignore '(^|/)ui/'",
    "build": "npm run build:js && npm run build:backends",
    "watch": "npm run build:js:dev -- --watch",
    "dist": "python -m build --sdist --wheel --outdir dist/",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css}\""
  },
  "author": "ludwig jonsson <dev-kaleido@protonmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@radix-ui/react-context-menu": "2.2.6",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-popover": "1.1.6",
    "@radix-ui/react-separator": "1.1.2",
    "@radix-ui/react-slot": "1.1.2",
    "@radix-ui/react-tooltip": "1.1.8",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "tailwind-merge": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@plotly/dash-component-plugins": "^1.2.3",
    "@plotly/webpack-dash-dynamic-import": "^1.2.0",
    "@tailwindcss/postcss": "4.1.8",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "postcss": "8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "react": "18.3.1",
    "react-docgen": "^5.4.3",
    "react-dom": "18.3.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "4.1.8",
    "ts-loader": "^9.5.1",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "peerDependencies": {
    "react": "16 - 19",
    "react-dom": "16 - 19"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
